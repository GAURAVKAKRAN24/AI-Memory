          <script> 
            function open_chatbot() {
              var chat_container = document.getElementById("chat-container");
              var main_button = document.getElementById("mainBtn");
              if (chat_container.style.display === "none") {
                  chat_container.style.display = "block";
                  main_button.innerHTML = '<i class="fas fa-times" style="font-size : 26px;padding: 6px 9px; "></i> ';
              } else {
                  chat_container.style.display = "none";  
                  main_button.innerHTML = '<i class="fas fa-robot" style="font-size : 30px padding: 8px 12px; "></i>'
              }
              }
          </script>

